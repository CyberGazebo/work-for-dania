<Window x:Class="invokertrainer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:invokertrainer"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="500"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Frame Name="Mainframe" Grid.Column="1" NavigationUIVisibility="Hidden"/>
        <TextBlock Text="Тренировка кастов на вокере" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,49,0,0"/>
        <Button x:Name="Hirezbtn" Content="Рекорд" Margin="10,341,10,95"/>
        <Button x:Name="Exitbtn" Content="Выход" Margin="40,439,40,31" Click="Exitbtn_Click"/>
    </Grid>
</Window>
